{
	"name" : "dmk/mklib",
	"description" : "Utilities for extensions.",
	"version" : "2.0.17",
	"type" : "typo3-cms-extension",
	"keywords" : ["TYPO3 CMS", "mvc"],
	"homepage" : "http://www.dmk-ebusiness.de/",
	"license" : "GPL-2.0+",
	"authors" : [
		{
			"name" : "DMK E-BUSINESS GmbH",
			"email" : "dev@dmk-ebusiness.de",
			"role" : "Developer"
		}
	],
	"support": {
		"email": "dev@dmk-ebusiness.de",
		"source": "https://github.com/DMKEBUSINESSGMBH/typo3-mklib",
		"issues": "https://github.com/DMKEBUSINESSGMBH/typo3-mklib"
	},
	"replace": {
		"mklib": "self.version",
		"typo3-ter/mklib": "self.version"
	},
	"require" : {
		"typo3/cms-core" : ">=4.5.0 <=8.7.99",
		"typo3-ter/rn-base" : ">=1.0.4"
	},
	"autoload": {
		"classmap": [
			"scheduler/", "exception/", "interface/", "model/", "treelib/", "util/", "validator/", "Classes/", "srv/",
			"action/", "abstract/", "cli/", "filter/", "marker/", "hooks/", "interface/", "mod1/", "search/", "soap/",
			"tca/", "view/", "repository/"
		]
	}
}
